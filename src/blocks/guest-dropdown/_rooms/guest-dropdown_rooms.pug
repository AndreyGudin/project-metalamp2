mixin guest-dropdown_rooms-dropdown(flabel,slabel,tlabel)
  .guest-dropdown_rooms-dropdown.iqdropdown
    p.iqdropdown-selection
    div.guest-dropdown_rooms-dropdown.iqdropdown-menu(data-hide="1") 
      .iqdropdown-menu-option(data-id="item1")
        div
          p.iqdropdown-item=flabel
      .iqdropdown-menu-option(data-id="item2")
        div
          p.iqdropdown-item=slabel
      .iqdropdown-menu-option(data-id="item3")
        div
          p.iqdropdown-item=tlabel